Alt & Shift:: {
    send("+!{o}")  ; Shift + Alt + O
    sleep(10)
}

Alt & z:: {
    send("+!{f}")  ; Shift + Alt + F
    sleep(10)
    send("+!{e}")  ; Shift + Alt + E
    sleep(10)
    send("+!{l}")  ; Shift + Alt + L
    sleep(10)
}

Alt & x:: {
    send("^+k")  ; Ctrl + Shift + K
    sleep(10)
}
