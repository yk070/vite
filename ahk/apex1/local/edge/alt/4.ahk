sc38 & Shift:: {
    send("+!{o}")  ; Shift + sc38 + O
    sleep(10)
}

sc38 & z:: {
    send("+!{f}")  ; Shift + sc38 + F
    sleep(10)
    send("+!{e}")  ; Shift + sc38 + E
    sleep(10)
    send("+!{l}")  ; Shift + sc38 + L
    sleep(10)
}

sc38 & x:: {
    send("^+k")  ; Ctrl + Shift + K
    sleep(10)
}
