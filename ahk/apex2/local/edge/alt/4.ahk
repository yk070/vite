Alt & Shift:: {
    Send("+!{o}")  ; Shift + Alt + O
    Sleep(10)
}

Alt & z:: {
    Send("+!{f}")  ; Shift + Alt + F
    Sleep(10)
    Send("+!{e}")  ; Shift + Alt + E
    Sleep(10)
    Send("+!{l}")  ; Shift + Alt + L
    Sleep(10)
}

Alt & x:: {
    Send("^+k")  ; Ctrl + Shift + K
    Sleep(10)
}
